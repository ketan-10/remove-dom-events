{
  "manifest_version": 2,

  "name": "Remove Events",
  "description": "Force Sites to Stop Events",
  "version": "0.0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],

      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "icons": { "128": "logo-big.png" },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
